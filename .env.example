# ============================================
# SynergyAI Environment Variables
# ============================================
# SECURITY WARNING: Never commit .env files!
# Copy this to .env and fill in your values
# ============================================

# ==========================================
# FRONTEND VARIABLES (VITE_* are PUBLIC)
# These will be bundled into the frontend!
# ==========================================

# Katana Network RPC (public endpoint, safe for frontend)
VITE_KATANA_RPC_URL=https://rpc.katana.network
VITE_KATANA_CHAIN_ID=747474

# Backend API endpoints
VITE_AGENT_BACKEND_URL=http://localhost:3001
VITE_IDENTITY_VERIFIER_API_URL=http://localhost:3001

# Block explorers
VITE_KATANA_EXPLORER_URL=https://katanascan.com

# ==========================================
# BACKEND/CONTRACTS VARIABLES (PRIVATE)
# ==========================================

# Katana Network RPC (can be same as frontend or premium endpoint)
KATANA_RPC_URL=https://rpc.katana.network

# CRITICAL: Private keys for deployment and agent operations
# NEVER commit these! Keep them in .env only!
DEPLOYER_PRIVATE_KEY=0x...your_deployer_private_key_here
AGENT_PRIVATE_KEY=0x...your_agent_private_key_here

# Smart Contract Addresses (fill after deployment)
REGISTRY_CONTRACT_ADDRESS=
AGENT_WALLET_IMPLEMENTATION_ADDRESS=

# Katana Token Addresses
SUSHI_ROUTER_ADDRESS=0x1b02dA8Cb0d097eB8D57A175b88c7D8b47997506
VBUSDC_TOKEN_ADDRESS=
WETH_TOKEN_ADDRESS=
KAT_TOKEN_ADDRESS=

# AI & APIs
OPENAI_API_KEY=sk-...your_openai_key_here
PRICE_FEED_URL=https://api.coingecko.com/api/v3/simple/price?ids=ethereum&vs_currencies=usd

# Privado ID (Polygon ID)
PRIVADO_ID_ISSUER_URL=
PRIVADO_ID_VERIFIER_URL=

# Backend Server
PORT=3001
NODE_ENV=development
